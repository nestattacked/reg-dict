{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "react",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "module": "commonjs",
    "target": "es5"
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "webpack/**/*.ts"]
}
